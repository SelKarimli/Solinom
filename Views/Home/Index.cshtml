@model List<FinalProject.MVC.Models.Room>
@{
    ViewData["Title"] = "Home Page";
    if (Model == null)
    {
        <div class="alert alert-danger">No rooms data available.</div>
        return;

    }
}

<section class="page-header"  style="position: relative; height: 80vh; overflow: hidden;" >
    <!-- Background Image -->
    <img src="~/assets/images/backgrounds/slider-1-1.jpg"
         alt="Hotel Background"
         style="position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                object-fit: cover;
                z-index: 0;">

        <!-- Dark Overlay -->
        <div style="position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                z-index: 1;
                background-color: rgba(0,0,0,0.5);">
        </div>

        <!-- Content Container -->
        <div style="position: relative;
                z-index: 2;
                height: 100%;
                display: flex;
                flex-direction: column;
                justify-content: center;
                align-items: center;
                text-align: center;
                color: white;
                padding: 20px;">

            <!-- Text Container with Background -->
            <div style="background-color: rgba(0, 0, 0, 0.4);
                   backdrop-filter: blur(5px);
                   padding: 30px 50px;
                   border-radius: 8px;
                   margin-bottom: 20px;
                   border: 1px solid rgba(255,255,255,0.1);">
                <!-- Main Heading -->
                <h1 style="font-size: 4rem;
                       margin-bottom: 1rem;
                       font-weight: 700;
 color:white;
                       text-transform: uppercase;
                       letter-spacing: 3px;
                       text-shadow: 2px 2px 4px rgba(0,0,0,0.5);">
                ENJOY LUXURY EXPERIENCE
                </h1>

                <!-- Subheading -->
                <p style="font-size: 1.8rem;
                     margin-bottom: 0;
color: #d4af37;
                     text-transform: uppercase;
                     letter-spacing: 2px;
                     text-shadow: 1px 1px 3px rgba(0,0,0,0.5);">
                THE BEST 5 STAR HOTEL
                
                </p>
            </div>

            <!-- Search Form -->
            <div style="background: rgba(255,255,255,0.9);
                   padding: 30px;
                   border-radius: 8px;
                   width: 80%;
                   max-width: 900px;
                   margin-top: 20px;
                   /* box-shadow: 0 5px 15px rgba(0,0,0,0.2);" */>
                <form asp-controller="Home" asp-action="SearchRooms" method="get">
                    <div style="display: grid;
                           grid-template-columns: repeat(5, 1fr) 120px;
                           gap: 15px;
                           align-items: end;">
                        <!-- Capacity -->
                        <div>
                            <label style="display: block;
                                     margin-bottom: 8px;
                                     text-align: left;
                                     color: #333;
                                     font-weight: 600;">Capacity</label>
                            <input type="number"
                                   name="capacity"
                                   placeholder="e.g. 2"
                                   style="width: 100%;
                                      padding: 10px;
                                      border: 1px solid #ddd;
                                      border-radius: 4px;">
                        </div>

                        <!-- Min Price -->
                        <div>
                            <label style="display: block;
                                     margin-bottom: 8px;
                                     text-align: left;
                                     color: #333;
                                     font-weight: 600;">Minimum</label>
                            <input type="number"
                                   name="minPrice"
                                   placeholder="$50"
                                   style="width: 100%;
                                      padding: 10px;
                                      border: 1px solid #ddd;
                                      border-radius: 4px;">
                        </div>

                        <!-- Max Price -->
                        <div>
                            <label style="display: block;
                                     margin-bottom: 8px;
                                     text-align: left;
                                     color: #333;
                                     font-weight: 600;">Maximum</label>
                            <input type="number"
                                   name="maxPrice"
                                   placeholder="$200"
                                   style="width: 100%;
                                      padding: 10px;
                                      border: 1px solid #ddd;
                                      border-radius: 4px;">
                        </div>

                        <!-- Check-in -->
                        <div>
                            <label style="display: block;
                                     margin-bottom: 8px;
                                     text-align: left;
                                     color: #333;
                                     font-weight: 600;">Check-in</label>
                            <input type="date"
                                   name="checkIn"
                                   style="width: 100%;
                                      padding: 10px;
                                      border: 1px solid #ddd;
                                      border-radius: 4px;">
                        </div>

                        <!-- Check-out -->
                        <div>
                            <label style="display: block;
                                     margin-bottom: 8px;
                                     text-align: left;
                                     color: #333;
                                     font-weight: 600;">Check-out</label>
                            <input type="date"
                                   name="checkOut"
                                   style="width: 100%;
                                      padding: 10px;
                                      border: 1px solid #ddd;
                                      border-radius: 4px;">
                        </div>

                        <!-- Search Button -->
                        <div>
                            <button type="submit"
                                    style="width: 100%;
                                       padding: 12px;
                                       background-color: #d4af37;
                                       color: white;
                                       border: none;
                                       border-radius: 4px;
                                       font-weight: 600;
                                       cursor: pointer;">
                                Search
                            </button>
                        </div>
                    </div>
                </form>
            @* </div> *@
        </div>
    </div>
    <!-- main-slider-Done -->

</section>
<!-- main-slider-end --> 
        


