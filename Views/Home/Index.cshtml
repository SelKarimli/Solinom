<!-- Feature Section Start -->
<section class="feature-five">
    <div class="container">
        <div class="sec-title text-center sec-title--two">
            <img src="~/assets/images/shapes/sec-title-s-1.png" alt="FEATURED ROOMS" class="sec-title__img">
            <h6 class="sec-title__tagline bw-split-in-right">FEATURED ROOMS</h6>
            <h3 class="sec-title__title bw-split-in-up">Discover Our Rooms</h3>
        </div>
        <div class="feature-five__carousel solinom-owl__carousel owl-carousel owl-theme solinom-owl__carousel--with-shadow" data-owl-options='{
             "items" : 1,
             "margin" : 0,
             "loop" : true,
             "smartSpeed" : 700,
             "nav" : true,
             "navText" : ["<span class=\"icon-right-arrow\"></span>","<span class=\"icon-right-arrow2\"></span>"],
                "dots": false,
                "autoplay": true,
                "responsive": {
                    "0": {
                        "items": 1
                    },
                    "768": {
                        "items": 2,
                        "margin": 30
                    },
                    "992": {
                        "items": 2,
                        "center": false,
                        "margin": 30
                    },
                    "1199": {
                        "items": 3,
                        "center": true,
                        "margin": 30
                    }
                }
            }'>

            @foreach (var room in Model)
            {
                    <div class="item">
                        <div class="feature-five-card wow fadeInUp" data-wow-duration='1500ms' data-wow-delay='000ms'>
                            <div class="feature-five-card__inner">
                                <div class="feature-five-card__thumb">
                                        <img src="~/imgs/rooms/@room.CoverImage" alt="@room.Name">
                                    <a asp-controller="Home" asp-action="Room1" asp-route-id="@room.Id">
                                        <p class="feature-five-card__feature">feature</p></a>
                                    <div class="feature-five-card__thumb__popup">
                                        <div class="feature-five-card__thumb__popup__love">
                                            <i class="far fa-heart"></i>
                                        </div>
                                        <a href="https://www.youtube.com/watch?v=4jnzf1yj48M" class="feature-five-card__thumb__popup__item video-popup">
                                            <i class="icon-video-camera-1-1"></i>
                                        </a>
                                                <a href="@Url.Content($"~/imgs/rooms/{room.CoverImage}")"
       class="feature-five-card__thumb__popup__item room-list-card-popup"
       data-gallery-options='{
            "items": [
                {
                    "src": "@Url.Content($"~/imgs/rooms/{room.CoverImage}")"
                }
            ],
            "gallery": {
                "enabled": true
            },
            "type": "image"
        }'>
        <i class="icon-photo-camera-1"></i>
    </a>
                                    </div>
                                </div>
                                <div class="feature-five-card__content">
                                    <h4 class="feature-five-card__content__title"><a asp-controller="Home" asp-action="Room1" asp-route-id="@room.Id">@room.Name</a></h4>
                                    <p class="feature-five-card__content__text"><a asp-controller="Home" asp-action="Room1" asp-route-id="@room.Id">@room.Description</a></p>
                                </div>
                                <div class="feature-five-card__content__number">
                                    <div class="feature-five-card__content__number__price">$@room.Price.ToString("0.00")/<span>night</span></div>
                                    <p class="feature-five-card__content__number__parson">Person: <strong>@room.Capacity</strong></p>
                                </div>
                            </div>
                        </div>
                    </div>
            }
        </div>
    </div>
    <div class="feature-five__shape" style="background-image: url(~/assets/images/shapes/feature-2-1.png);"></div>
</section>
<!-- Feature Section End -->
